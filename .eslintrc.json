{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "camelcase": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "indent": 0,
        "consistent-return": 0,
        "max-len": 1,
        "no-shadow": 0,
        "prefer-arrow-callback": 0,
        "guard-for-in": 0,
        "no-restricted-syntax": 0,
        "no-await-in-loop": 0,
        "no-use-before-define": 1
    }
}